@import url(https://fonts.googleapis.com/css?family=Permanent+Marker|Kalam|Annie+Use+Your+Telescope);
.swiper-container {
    width: 768px;
    height: 1024px;
  background:#fff;
  overflow:hidden;
}
.processus {
  position:relative;
  background:#fff;
  font-family: 'Permanent Marker', sans-serif;
  font-size:12px;
  color:#222;
}
.processus h2 {
  margin:1em 1em 2em 7em;
  position:relative;
  display: inline-block;
  height:calc(6em + 40px);
}
.processus h2 strong {
  font-size:300%;
  font-weight:bold;
  transform-origin:0 0;
  transform:translateY(calc(3em - 60px)) rotate(-5deg)  translateX(-0.1em);
  display: block;
  text-transform:uppercase;
}
.processus h2 strong:after {
  content:"";
  width: 100%;
  height:30px;
  display:block;
  background:url("data:img/png;base64,") no-repeat 0 0;
  background-size:contain;
}
.processus h2 span {
  font-size:100%;
  position:absolute;
  left:100px;
  bottom:0;
  text-align:right;
  font-family: 'Annie Use Your Telescope', cursive;
  font-size:150%;
}
.processus ul {
  margin: 0 1em;
}
.processus li {
  margin:1em 0;
  position:relative;
  overflow:hidden;
  display:table-row;
  border:1em solid rgba(0,0,0,0);
}
.processus .numero {
  display:table-cell;
  text-align:center;
  vertical-align:top;
  /*font-family: 'Shadows Into Light', cursive;*/
}
.processus .numero strong {
  width:1.5em;
  height:1.5em;
  line-height:1.5em;
  font-weight:bold;
  font-size:300%;
  border-radius:50%;
  background:#d0021b;
  color:#fff;
  display:block;
  margin:0 0.5em 0.5em 0;
  transition:all 0.25s ease-in-out;
}
.processus li.current .numero strong {
  transform:scale(1.2);
}
.processus .tache {
  display:table-cell;
}
.processus .tache h3 {
  color:#4a90f9;
  background:#fae80c;
  font-weight:bold;
  padding:0.3em;
  font-size:200%;
  display:inline-block;
  transition:all 0.25s ease-in-out;
  border-radius:0.2em;
  font-family:'Annie Use Your Telescope', cursive;
}
.processus li.current .tache h3 {
  background:#d0021b;
  color:#fff;
}
.processus .tache p {
  color: #000;
  padding:0.2em;
  margin: 0.5em 0 1em 0.5em;
  font-family: 'Annie Use Your Telescope', cursive;
  font-size:130%;
  line-height:140%;
  background:rgba(0,0,0,0.0);
  transition:all 0.25s ease-in-out;
}
.processus li.current .tache p {
  background:rgba(0,0,0,0.1);
}